import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.println("Enter the number: ");
        int a=in.nextInt();
        System.out.println("OUTPUT IS: ");
        int sum=1;
        for(int i=1;i<=a;i++){
            sum*=i;
        }
        System.out.print(sum);
    }
}

Enter the number: 
5
OUTPUT IS: 
120
